<script src="https://cdnjs.cloudflare.com/ajax/libs/prism/1.24.1/prism.min.js"></script>


    <link rel="icon" type="image/png" href="/assets/favicon/favicon-96x96.png" sizes="96x96" />
    <link rel="icon" type="image/svg+xml" href="/assets/favicon/favicon.svg" />
    <link rel="shortcut icon" href="/assets/favicon/favicon.ico" />
    <link rel="apple-touch-icon" sizes="180x180" href="/assets/favicon/apple-touch-icon.png" />
    <meta name="apple-mobile-web-app-title" content="Andre Sanz" />
    <link rel="manifest" href="/assets/favicon/site.webmanifest" />

    {% if site.googleAnalytics %}
    <script async src="https://www.googletagmanager.com/gtag/js?id={{ site.googleAnalytics }}"></script>

    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());
        gtag('config', '{{ site.googleAnalytics }}');
    </script>

    {% endif %}

    <button id="backToTopBtn" onclick="scrollToTop()" style="border: 1px solid grey; color: grey"><i class="fa-solid fa-arrow-turn-up" style="padding-bottom: 4px; vertical-align: middle; font-size: 18px;" title="Back to top..."></i></button>
    
